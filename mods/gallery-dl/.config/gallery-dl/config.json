{
  "extractor": {
    "mangadex": {
      "lang": "en",
      "directory": ["{manga} vol. {volume} [mangadex]"],
      "filename": "c{chapter:>03}{chapter_minor}{title:? //} p{page:>03} [{group:J, /}].{extension}"
    },
    "pixiv": {
      "filename": "{title} [{user[name]}] [pixiv {filename}].{extension}",
      "ugoira": "original"
    },
    "twitter": {
      "filename": "{date:%Y-%m-%d}{content:? //} [@{user[name]}] [twitter {tweet_id}_p{num}].{extension}"
    },
    "base-directory": ".",
    "directory": [],
    "path-restrict": {
      "\\": "⧹",
      "/": "⧸",
      "|": "￨",
      ":": "꞉",
      "*": "∗",
      "?": "？",
      "\"": "″",
      "<": "﹤",
      ">": "﹥"
    },
    "path-strip": "windows"
  },
  "downloader": {
    "mtime": false
  }
}
